	email = arneschreuder@icloud.com
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
[core]
    excludesfile = ~/.gitignore
    editor = "nvim"
[help]
    autocorrect = 1
[push]
    default = simple
[init]
	defaultBranch = main
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
